{
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "rules": {
        "require-jsdoc": 0,
        "linebreak-style": 0,
        "brace-style": 0,
        "padded-blocks": 0,
        "max-len": 0,
        "quote-props": 0,
        "object-curly-spacing": 0,
        "no-multiple-empty-lines": 0,
        "guard-for-in": 0,
        "eqeqeq": 0,
        "jsx-a11y/accessible-emoji": 0
    }
}