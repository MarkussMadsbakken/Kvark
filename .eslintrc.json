{
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "rules": {
        "require-jsdoc": 0,
        "linebreak-style": 0
    }
}